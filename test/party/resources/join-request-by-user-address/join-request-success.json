{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "message": {
            "type": "string"
        },
        "meta": {
            "type": "object",
            "properties": {
                "perPage": {
                    "type": "integer"
                },
                "page": {
                    "type": "integer"
                },
                "totalPage": {
                    "type": "integer"
                },
                "total": {
                    "type": "integer"
                }
            },
            "required": [
                "perPage",
                "page",
                "totalPage",
                "total"
            ]
        },
        "data": {
            "type": "array",
            "items": [
                {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "party": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                },
                                "type": {
                                    "type": "string"
                                },
                                "purpose": {
                                    "type": "string"
                                },
                                "distribution": {
                                    "type": "string"
                                },
                                "image_url": {
                                    "type": "null"
                                },
                                "is_public": {
                                    "type": "boolean"
                                },
                                "min_deposit": {
                                    "type": "integer"
                                },
                                "max_deposit": {
                                    "type": "integer"
                                },
                                "total_member": {
                                    "type": "integer"
                                },
                                "total_fund": {
                                    "type": "integer"
                                },
                                "owner_id": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "string"
                                },
                                "updated_at": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "id",
                                "name",
                                "type",
                                "purpose",
                                "distribution",
                                "image_url",
                                "is_public",
                                "min_deposit",
                                "max_deposit",
                                "total_member",
                                "total_fund",
                                "owner_id",
                                "created_at",
                                "updated_at"
                            ]
                        },
                        "status": {
                            "type": "string"
                        },
                        "accepted_at": {
                            "type": "null"
                        },
                        "rejected_at": {
                            "type": "null"
                        }
                    },
                    "required": [
                        "id",
                        "party",
                        "status",
                        "accepted_at",
                        "rejected_at"
                    ]
                }
            ]
        }
    },
    "required": [
        "message",
        "meta",
        "data"
    ]
}